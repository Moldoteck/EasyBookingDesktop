        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TIP"
        mc:Ignorable="d"
        Title="HouseDetails" Height="600" Width="800">
    <Grid>
        <Button x:Name="logout" Content="Log out" Margin="650,10,20,0" VerticalAlignment="Top" Width="110" Height="35" Background="#FF96DCFF" FontFamily="Script MT Bold" FontSize="20" BorderThickness="1" Click="logout_Click"/>
        <Button x:Name="back" Content="Back to search" Margin="510,10,140,0" VerticalAlignment="Top" Width="130" Height="35" Background="#FF96DCFF" FontFamily="Script MT Bold" FontSize="20" BorderThickness="1" Click="back_Click"/>
        <Grid x:Name="supergrid" HorizontalAlignment="Left" Height="492" Margin="10,67,0,0" VerticalAlignment="Top" Width="756"/>

    </Grid>
</Window>
